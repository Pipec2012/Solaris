.header
    color:#fff
   //  padding-top: 0.2vw
    a
        color: #fff

.header__background
	background: url("./img/Nebula.jpg") center no-repeat
	background-size: cover
	position: absolute
	z-index: -2
	height: 100vh
	width: 100%  
	

.logo
	position: absolute
	top: 1vw
	left: 4vw

.menu-wrap
	max-width 1400px
	display: flex
	flex-wrap: wrap
	
	justify-content center
	
	// margin: auto
	// align-items: center 

.menu
	display: flex
	// left 25%
	
	z-index: 9999999
	width 50%
	left 25%
	justify-content center
	margin: auto
	font-size: 1.4rem
	font-weight 200
	text-transform: uppercase
	letter-spacing: 3px
	padding-top: 1vw
	position: fixed
	top: 0
	
	text-align: center;

.menu:after
	content: ""
	background: rgba(31, 32, 65, 0.25)
	height 50px
	width: 100%
	border: 1px
	z-index: -1
	position: fixed
	top: 0

.menu__link
	cursor: pointer


.menu__link:hover + .cssload-wrap
    opacity 1


.menu__item
	padding: 0 15px

#menu li a, #menu li.active a + .cssload-wrap{
   opacity 1;
}
 
/*Стиль элемента списка когда активен соответствующий блок*/
#menu li.active + .cssload-wrap{
	opacity 1;
}

/*   --- Контакты  --- */ 

.submenu
	margin-top: 0.5vw
	opacity: 0
	visibility: hidden
	background: white
	box-shadow: 0 0 5px black
	border-radius: 10px
	padding: 20px 40px
	position: absolute
	font-size: 1.1rem
	font-weight: bold
	text-align: left
	display: flex
	flex-wrap: wrap
	margin-left: -4vw
	// top: 2vw
	letter-spacing: 0

.submenu__link
	color: black
	cursor: pointer
	margin-left: 0.5vw
li > i
	width: 25px
.fa-phone-volume
	color: #0884ee
.fa-whatsapp
	color: #07be1f
.fa-envelope
	color: #ff6f6f

.submenu__item
	margin-top: 1vw

.submenu__item:first-child
	margin-top: 0

.menu__item:hover .submenu
	opacity: 1
	visibility: visible
	transition-duration: 0.9s
	margin-bottom: 1vw


