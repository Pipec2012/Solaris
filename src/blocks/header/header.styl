@keyframes orbita
	from {-webkit-transform: rotate(0deg);}
	to {-webkit-transform: rotate(360deg);}

.fa-bullseye
	color: #ff3939
	// text-shadow: 0 -1px rgba(0,0,0,.8);
	-webkit-animation: glowing 2.5s linear infinite
	animation: glowing 2.5s linear infinite
	position: absolute
	font-size: 1.1rem
	left 47vw
	top: 13vh
	z-index: 5
	opacity: 0.9
	border-radius: 50%

// @keyframes pulsate{50%{color:#880000;text-shadow:0 1px rgba(0,0,0,.4),0 0 8px #f03000,0 0 8px #f80000;}}
@keyframes glowing
//   0% { border-radius: 50%;color: #B20000; -webkit-box-shadow: 0 0 4px #B20000; }
	50% { border-radius: 50%;color: #FF0000; -webkit-box-shadow: 0 0 2px 12px #ff3737; }
	60%{opacity: 0;}
	100% { border-radius: 50%;color: #ff3939; -webkit-box-shadow: 0 0 4px #B20000; opacity: 0;}

.location_pop-up
	opacity: 0
	visibility: hidden
	z-index: 6;
	position: relative

.location_pop-up__text
	position: absolute
	font-size: 1rem
	// font-weight: italic
	left -230px
	top: 2.5vw
	z-index: 5
	border: 1px solid
	padding: 10px 10px
	border-radius: 10px
	width: 15vw
	text-align: center
	// font-style: italic
	
	
.location_pop-up__text:before
	content: ""
	width: 1px
	height: 100px
	transform: rotate(95deg)
	left: 310px
	// top 0.5vw
	z-index: 5
	background: white
	position: absolute
.location-marker
	cursor: pointer
.location-marker:hover + .location_pop-up
	opacity 0.8
	visibility: visible
	z-index: 6
	-webkit-animation:zoomIn linear 0.8s
	animation:zoomIn linear 0.8s

// .location-marker:active + .location_pop-up
// 	opacity 0.8
// 	// animation: zoomIn 0.8s linear;
// 	z-index: 6
// 	-webkit-animation:zoomIn linear 0.8s
// 	animation:zoomIn linear 0.8s

@keyframes zoomIn {
  0% {
	opacity: 0;
	-webkit-transform: scale3d(0.3, 0.3, 0.3);
	transform: scale3d(0.3, 0.3, 0.3);
	
  }
  33.3% {
    -webkit-transform: skewX(6.25deg) skewY(6.25deg);
    transform: skewX(6.25deg) skewY(6.25deg);
  }

  44.4% {
    -webkit-transform: skewX(-3.125deg) skewY(-3.125deg);
    transform: skewX(-3.125deg) skewY(-3.125deg);
  }

  55.5% {
    -webkit-transform: skewX(1.5625deg) skewY(1.5625deg);
    transform: skewX(1.5625deg) skewY(1.5625deg);
  }

  66.6% {
    -webkit-transform: skewX(-0.78125deg) skewY(-0.78125deg);
    transform: skewX(-0.78125deg) skewY(-0.78125deg);
  }

  77.7% {
    -webkit-transform: skewX(0.390625deg) skewY(0.390625deg);
    transform: skewX(0.390625deg) skewY(0.390625deg);
  }

  100% {
    -webkit-transform: skewX(-0.1953125deg) skewY(-0.1953125deg);
    transform: skewX(-0.1953125deg) skewY(-0.1953125deg);
  }
}




